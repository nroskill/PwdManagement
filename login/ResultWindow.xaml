<Window x:Class="PwdManagement.login.ResultWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="提示"
        Height="200" Width="400"
        WindowStyle="None"
        MouseLeftButtonDown="Window_MouseLeftButtonDown"
        ResizeMode="CanMinimize"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Name="lb" VerticalAlignment="Top" HorizontalAlignment="Stretch" Height="120" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="#F14B31" Foreground="White">
            <TextBlock Name="tb" Text="您注册的用户已存在" FontFamily="CloudMeiHeiGBK" FontSize="24"  MaxWidth="350" TextWrapping="Wrap" Background="Transparent"/>
        </Label>
        <Button VerticalAlignment="Bottom" HorizontalAlignment="Center" Height="35" Width="120" Content="返回" FontSize="15" FontFamily="黑体" Foreground="White" Background="#F14B31" Margin="0,0,0,20" Name="btn" Click="btn_Click" BorderBrush="Transparent"/>
    </Grid>
</Window>
